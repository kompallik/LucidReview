{
  "name": "@lucidreview/backend",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm --out-dir dist",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "migrate": "knex --knexfile src/db/knexfile.ts migrate:latest",
    "migrate:make": "knex --knexfile src/db/knexfile.ts migrate:make",
    "ingest:cms": "tsx ../../scripts/ingest-cms-policies.ts",
    "generate:criteria": "tsx ../../scripts/generate-criteria-from-cms.ts",
    "init:hapi-fhir": "tsx ../../scripts/init-hapi-fhir.ts"
  },
  "dependencies": {
    "@lucidreview/shared": "workspace:*",
    "@lucidreview/mcp-server": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@aws-sdk/client-bedrock-runtime": "^3.817.0",
    "@aws-sdk/credential-providers": "^3.817.0",
    "fastify": "^5.3.3",
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.0",
    "@fastify/jwt": "^9.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "@fastify/static": "^8.1.0",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.0",
    "knex": "^3.1.0",
    "mysql2": "^3.14.1",
    "pino": "^9.7.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "@types/bcryptjs": "^2.4.6",
    "vitest": "^3.1.1"
  }
}
